{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./www",
    "rootDir": "./www",
    "removeComments": false,
    "downlevelIteration": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "include": [
    "./www"
  ],
  "exclude": []
}
